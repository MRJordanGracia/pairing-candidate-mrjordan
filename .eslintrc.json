{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "plugin:import/recommended",
    "airbnb-base",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": 0,
    "import/no-unresolved": 0
  },
  "settings": {
    "import/extensions": [".mjs", ".js"],
    "import/resolver": "node"
  }
}
